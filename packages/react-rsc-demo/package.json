{
  "name": "react-rsc-demo",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "type": "module",
  "types": "./dist/types/App.d.ts",
  "scripts": {
    "build": "pnpm run ts:check && pnpm run build:server && pnpm run build:client && pnpm run ts:declaration",
    "build:client": "node --loader ts-node/esm node_modules/webpack-cli/bin/cli.js --config ./config/webpack.config.client.ts && tsx ./config/fix-manifest-paths.ts",
    "build:server": "node --loader ts-node/esm node_modules/webpack-cli/bin/cli.js --config ./config/webpack.config.server.ts",
    "start": "pnpm run build && tsx --conditions react-server server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ts:check": "tsc --noEmit",
    "ts:declaration": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.25.0",
  "sideEffects": [
    "src/**/*.tsx",
    "src/**/*.ts"
  ],
  "dependencies": {
    "express": "^5.2.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@types/webpack": "^5.28.5",
    "@types/webpack-node-externals": "^3.0.4",
    "babel-loader": "^10.0.0",
    "html-webpack-plugin": "^5.6.6",
    "react-server-dom-webpack": "^19.2.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
